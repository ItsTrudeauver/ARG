    <script>
        const puzzleAnswers = {
            1: "WALKENHURST",
        };

        const puzzleHints = {
            1: "He who sacrificed sanity, only for the emptiness to beget regrets.",
           
        };

        const attemptCounts = {};

        function checkPuzzle(puzzleNum) {
            const input = document.getElementById(`puzzle${puzzleNum}-input`).value.toUpperCase();
            const hintBox = document.getElementById(`puzzle${puzzleNum}-hint`);

            attemptCounts[puzzleNum] = (attemptCounts[puzzleNum] || 0) + 1;

            if (input === puzzleAnswers[puzzleNum]) {
                // Puzzle solved
                document.querySelector(`.scroll-section:nth-child(${puzzleNum}) .section-content`).style.display = 'block';
                document.querySelector(`.scroll-section:nth-child(${puzzleNum}) .puzzle-interface`).remove();
                // Add animation effects
            } else {
                // Show hint after 3 attempts
                if (attemptCounts[puzzleNum] >= 3) {
                    hintBox.style.maxHeight = '50px';
                    hintBox.innerHTML = `HINT: ${puzzleHints[puzzleNum]}`;
                }
                // Add glitch effect animation
            }
        }
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                const section = entry.target;
                const visibleRatio = entry.intersectionRatio;

                section.style.opacity = 0.3 + (visibleRatio * 0.7);
                section.style.transform = `scale(${0.95 + (visibleRatio * 0.05)}) 
                                        translateZ(${-100 + (visibleRatio * 100)}px)`;

                if (visibleRatio > 0.7) {
                    section.classList.add('active');
                } else {
                    section.classList.remove('active');
                }
            });
        }, {
            threshold: Array.from({ length: 100 }, (_, i) => i * 0.01),
            rootMargin: '-50px 0px -50px 0px'
        });

        document.querySelectorAll('.scroll-section').forEach(section => {
            observer.observe(section);
        });

        function toggleSection(header) {
            const content = header.nextElementSibling;
            const isExpanding = !content.classList.contains('expanded');

            content.classList.toggle('expanded');

            if (isExpanding) {
                const section = header.parentElement;
                section.scrollIntoView({
                    behavior: 'smooth',
                    block: 'center'
                });
                section.classList.add('active');
            }
        }

        // Initialize first section as expanded
        document.querySelector('.scroll-section:first-child .section-content')
            .classList.add('expanded');
    </script>
